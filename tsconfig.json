    {
        "compilerOptions": {
            "lib":["es2015","ES6", "dom", "es2018.promise"],
            "target": "ESNext",
            "strict": true,
            "module": "commonjs",
            "sourceMap": true,
            "resolveJsonModule": true,
            "types": [
                "node", "jest", "@testing-library/jest-dom"
                 
            ],
            "baseUrl": ".",
            "paths": {
                "@pages/*": [
                    "pages/*"
                ],
                "@fixtures/*": [
